{
    "name": "components-uxpin",
    "version": "0.8.2",
    "private": true,
    "description": "Generated components for UX Pin",
    "license": "MIT",
    "main": "./dist/bundle.js",
    "scripts": {
        "build-uxpin": "webpack --config ./webpack.uxpin.config.js",
        "push:uxpin": "./node_modules/.bin/uxpin-merge push --webpack-config ./webpack.uxpin.config.js --config uxpin.config.js --wrapper ./src/wrappers/Wrapper.js",
        "start:uxpin": "uxpin-merge --webpack-config ./webpack.uxpin.config.js --config uxpin.config.js --wrapper ./src/wrappers/Wrapper.js --disable-tunneling",
        "start:uxpin-server": "uxpin-merge server --webpack-config ./webpack.uxpin.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/types": "^7.8.3",
        "@uxpin/merge-cli": "^2.4.1",
        "babel-loader": "^8.0.6",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.4",
        "webpack": "^4.41.6",
        "webpack-cli": "^3.3.11"
    },
    "dependencies": {
        "components": "^0.0.0",
        "use-custom-element": "^1.0.5"
    }
}
